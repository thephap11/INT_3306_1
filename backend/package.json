{
  "name": "football-management-backend",
  "version": "0.1.0",
  "type": "module",
  "main": "src/server.js",
  "scripts": {
    "dev": "node src/server.js",
    "start": "node src/server.js",
    "sync-config": "node sync-config.cjs",
    "reset-config": "node reset-config.cjs",
    "db:migrate": "node sync-config.cjs && sequelize-cli db:migrate && node reset-config.cjs",
    "db:migrate:undo": "sequelize-cli db:migrate:undo",
    "db:migrate:reset": "node sync-config.cjs && sequelize-cli db:migrate:undo:all && sequelize-cli db:migrate && node reset-config.cjs",
    "db:migrate:status": "sequelize-cli db:migrate:status",
    "create-admin": "node create-admin.js",
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest",
    "test:watch": "NODE_OPTIONS='--experimental-vm-modules' jest --watch",
    "test:coverage": "NODE_OPTIONS='--experimental-vm-modules' jest --coverage",
    "test:unit": "NODE_OPTIONS='--experimental-vm-modules' jest --testPathPattern=unit",
    "test:integration": "NODE_OPTIONS='--experimental-vm-modules' jest --testPathPattern=integration"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.3",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "node-fetch": "^3.3.2",
    "nodemailer": "^7.0.11",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.35.2"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "jest": "^29.7.0",
    "node-mocks-http": "^1.17.2",
    "sequelize-cli": "^6.6.2",
    "supertest": "^6.3.4"
  }
}
